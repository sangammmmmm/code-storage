#include<iostream>
using namespace std;
void leftrotate(int arr[],int n,int d)

{
d=d%n;
int temp[d];
for (int i = 0; i <d; i++)
{
    /* code */temp[i]=arr[i];
}

for (int i = d; i <n; i++)
{
    /* code */arr[i-d]=arr[i];
}
for (int i = n-d; i <n ; i++)
{
    /* code */arr[i]=temp[i-(n-d)];
}

}
    
}

int main()
{
    int n;
    cin>>n;
    int arr[n];
    int d;
    cin>>d;
    for (int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    leftrotate(arr,n,d);

    for(int i=0;i<n;i++)
    {
        cout<<arr[i];
    }
};       
