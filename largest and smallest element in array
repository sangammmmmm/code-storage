#include<iostream>
using namespace std;
int main()
{
    // largest element in the array 
    int n;
    cin>>n;
    int arr[n];
    for (int  i = 0; i < n; i++)
    {
     cin>>arr[i];
    }
    
    int temp=arr[0];
    for(int i = 0;i<n;i++)
    {
       if(arr[i]>temp)
       {
        temp = arr[i];
       } 
    
    }
    cout<<temp<<endl;

    // smalest element in array
    int smallest = arr[0];
    for (int i = 0; i < n; i++)
    {
    if(arr[i]<smallest)
    {
        smallest = arr[i];
    }
    }
    cout<< smallest<< endl;
    
};        
